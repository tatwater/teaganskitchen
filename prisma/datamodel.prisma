type User {
  id: ID! @id
  name: String!
}
type Ingredient {
  id: ID! @id
  name: String!
}
type Recipe {
  id: ID! @id
  name: String!
  url: String! @unique
  type: String!
}
type Meal {
  id: ID! @id
}
type Event {
  id: ID! @id
  name: String!
  url: String! @unique
  starred: Boolean @default(value: false)
  description: String!
  startTime: DateTime!
  endTime: DateTime!
}
